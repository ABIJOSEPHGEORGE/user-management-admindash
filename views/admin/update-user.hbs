         <div class="update-user pt-5 mt-5">
            <h3 class="text-center text-black text-decoration-underline">Update Users</h3>
            <p class="bg-success text-white rounded text-center">{{success}}</p>
        </div>
        
        <div class="board">
            <table width="100%">
                <thead>
                     <tr>
                        <td>username</td>
                        <td>email</td>
                        <td>Action</td>
                    </tr>
                </thead>
                <tbody>
                    {{#each data}}
                    <tr>
                        <td class="people">
                            <div class="people-de">
                                <h5>{{this.username}}</h5>
                            </div>
                        </td>
                        <td class="email">
                            <h5>{{this.email}}</h5>
                        </td>
                        <td class="edit"><a href="/admin/update-form?id={{this.id}}">Update</a></td>
                    </tr>
                {{/each}}

                </tbody>
            </table>
        </div>
        <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
    <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>